#include <iostream>
using namespace std;

int main()
{
    float x;
    float y;
    float z;
    
    cout << "x:";
    cin >> x;
    cout << "y:";
    cin >> y;
    cout << "z:";
    cin >> z;
    float sum = x + y + z;
    
    float avrage = sum / 3;
    
    cout << avrage;
    return 12;
}

x:12.3
y:18.3
z:20
